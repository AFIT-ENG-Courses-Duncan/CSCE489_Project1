{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "C/C++ Debug (gdb Attach)",
            "type": "cppdbg",
            "request": "attach",
            "program": "${workspaceFolder}/myshell",
            "processId": "${command:pickProcess}",
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "sourceFileMap": {
                "/build/glibc-*": "/usr/src/glibc-2.31"
            }
        },
        {
            "name": "(gdb) Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/myshell",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "preLaunchTask": "build",
            "sourceFileMap": {
                "/build/glibc-*": "/usr/src/glibc-2.31"
            }
        }
    ]
}
